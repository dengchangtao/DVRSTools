geom_rect(colour = "grey30", show_guide = FALSE) +
coord_polar(theta = "y") +
xlim(c(0, 4)) +
theme_bw() +
theme(panel.grid=element_blank()) +
theme(axis.text=element_blank()) +
theme(axis.ticks=element_blank()) +
geom_text(aes(x = 3.5, y = ((ymin+ymax)/2), label = type)) +
xlab("") +
ylab("")
